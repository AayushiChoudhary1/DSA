#include <bits/stdc++.h>
using namespace std;


int display(int a[], int idx,int n){
	
	if(idx == n){
		return 0;
	}
	
	
	cout<<a[idx]<<endl;
	display(a,idx+1,n);
}

int main(){
	
	int n;
	cout<<"enter the number: "<<endl;
	cin>>n;
	/*-------------------------------------------*/
	int arr[n];
	for(int i=0; i<n; i++){
		cin>>arr[i];
	}
	
	display(arr,0,n);
}
