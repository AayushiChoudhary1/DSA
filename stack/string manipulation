  int removeConsecutiveSame(vector <string > v) 
    {
       stack<string> s;
       int i=0;
       
       while(i<v.size())
       {
           if(s.size()==0)
           s.push(v[i]);
           else
           {
               if(s.top()==v[i])
               s.pop();
               else
               s.push(v[i]);
           }
           
           i++;
       };
       
       return s.size();
    } 
