string merge (string S1, string S2)
{
    string S3="";
    int i=0, j=0;
    
    while(i<S1.size() && j<S2.size()){
        S3 += S1[i];
        S3 += S2[j];
        i++;
        j++;
    }
    
    while(i<S1.size()){
        S3 += S1[i];
        i++;
    }
    
    while(j<S2.size()){
        S3 += S2[j];
        j++;
    }
    
    return S3;
}
